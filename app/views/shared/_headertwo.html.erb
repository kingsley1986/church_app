<%= render "sermons/index" %>
	<section class="section-padding gray-bg">
		<div id="fh5co-about-us" data-section="about" style="margin-top:20px;">
			<div class="container">
				<div class="row row-bottom-padded-lg" id="about-us">
					<div class="col-md-12 section-heading text-center">
						<h2 class="to-animate" style="   display: inline-block;
						   font-family: 'Gabriela', serif;
						   color: #FFF;
						   font-size: 1.1em;
						   border-radius: 20px;
						   background: rgba(43, 166, 203, 0.9);
						   padding: 0.5em 0.2em 0.2em 0.2em;">Parish Pastor</h2>
					</div>
					<div class="col-md-8 to-animate">
						<% Page.all.each do |page| %>
						<% if page.page_type == "About_Parish_pastor" %>
							<% page.pictures.each do |page_pc| %>
								<%= image_tag page_pc.image.url(:large), class:"img-responsive img-rounded", alt:"Free HTML5 Template" if page_pc.image? %>
							<% end %>
						<% end %>
					<% end %>
					</div>
					<div class="col-md-4 to-animate" style="margin-top:-10px;">
						<% Page.all.each do |page| %>
						<% if page.page_type == "About_Parish_pastor" %>
							<p><%= truncate(page.body, length: 410) %></p>
						<% end %>
					<% end %>
						<p><a href="/pages/filter/about_parish_pastor" class="btn btn-primary">Read More</a></p>
					</div>
				</div>
	</section>
