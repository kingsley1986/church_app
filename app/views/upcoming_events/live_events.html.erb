<div class="page_wrapper_upcoming_event" style="font-size:15px; margin-bottom:15px; padding-left:30px; padding-right:30px; text-align: center; margin: 0 auto; background: rgba(54, 25, 25, .5)" >
  <didv class="content" >
    <% @lives.each do |live| %>
    <hr class="devidepost">
        <div class="upcoming_title" style="background-color:white; font-weight:bold">
          <%= live.title %>
        </div>
        <div class="upcomings_body">
          <%= live.body %>
        </div>
      </br>
        <div class="starting_date_class"><%= upcoming_event.starting_date.strftime("Starting on:  %Y-%m-%d %H:%M:%S") %></div>
      </br>
        <div class="end_date_class"><%= upcoming_event.end_date.strftime("Ends on:  %Y-%m-%d %H:%M:%S") %></div>
      </br>
        <div class="img img1.imgAlbum" >
          <% upcoming_event.pictures.each do |pic, remov| %>
            <%= image_tag(pic.image.url(:medium), class: "img-responsive") if pic.image? %>
          <% end %>
        </div>
    <% end %>
  </div>
</div>
