<% @images.in_groups_of(5, false).each do |group| %>
  <div class='row'>
    <% group.each do |imgg| %>
      <div class='col-md-2'>
				<div id="lightbox">
					<ul>
						<li>
        			<%= (image_tag imgg.image.url(:square), class:"img-responsive", datalarge:  imgg.image.url(:large))  %>
							<div class="image_title">
								<h5 class="title"><%= imgg.image_title %></h5>
							</div>
						</li>
					</ul>
				</div>
      </div>
    <% end %>
  </div>
<% end %>
